    </Sequence.Variables>
    <ui:ForEachRow CurrentIndex="{x:Null}" DataTable="[in_KisconUsers]" DisplayName="For Each Row - Credential 추가 " sap2010:WorkflowViewState.IdRef="ForEachRow_1">
